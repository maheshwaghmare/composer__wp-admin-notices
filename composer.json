{
    "name": "maheshwaghmare/wp-admin-notices",
    "description": "Mange the WordPress admin notices.",
    "authors": [
        {
            "name": "Mahesh Waghmare",
            "email": "mwaghmare7@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.16|6.*",
        "squizlabs/php_codesniffer": "3.*",
        "wp-coding-standards/wpcs": "^2.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "examples/"
        ],
        "classmap": [
            "php/index.php"
        ]
    }
}